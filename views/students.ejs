<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <a href="/">Home</a>
    <a href="/teachers">Teachers</a>
    <a href="/subjects">Subjects</a>
    <a href="/students">Students</a>
    <h1>Daftar Students</h1>
    <a href="students/add">add Student</a>
    <br><br>
    <table border="1">
      <tbody>
        <th>No</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Actions</th>
        <th>Options</th>
        <% var i=1; data.forEach(d=> {%>
        <tr>
          <td><%=i++%></td>
          <td><%=d.first_name%></td>
          <td><%=d.last_name%></td>
          <td><%=d.email%></td>
          <td><a href="students/<%=d.id%>/addsubject">Add Subject</a></td>
          <td><a href="students/edit/<%=d.id%>">Edit</a> |
            <a href="students/delete/<%=d.id%>" onclick="return confirm('delete this student?')">Delete</a></td>
        </tr>
        <%})%>
      </tbody>
    </table>

  </body>
</html>
