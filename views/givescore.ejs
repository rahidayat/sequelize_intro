<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <%- include layout/link %>
    <%- include layout/title %>
  </head>

  <body>
    <%- include layout/nav-bar %>
    <h1>Input Score</h1>
    <form action="/subjects/<%=conj[0].id%>/givescore" method="post">
      Student Name: <%=conj[0].Student.first_name%> <%=conj[0].Student.last_name%> <br>
      Subject Name: <%=conj[0].Subject.subject_name %> <br>

      Score : <input type="text" name="score"><br>

      <button type="submit" name="">Add score</button>
      <br><br>
    </form>
    <%- include layout/script %>

  </body>
</html>
